<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.StackPane?>


<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.deadlyspeed.gameManager.Lobby" stylesheets="/styles/lobby.css">
    <children>
      <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" spacing="20.0" style="-fx-background-color: #000000;">
         <children>
           <Text text="Deadly Speed" />
           <Button onAction="#quickLogIn" text="Quick Game" />
           <Button onAction="#getLeagueLobby" text="Champions League" />
           <Button onAction="#quitGame" text="Quit" />
         </children>
    </children>
</StackPane>
